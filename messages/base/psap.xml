  <!-- Connection rejection messages -->
  <message id="50" name="PeerRejected" severity="error">
    <text>Peer rejected presentation connection</text>
    <doc>
       <description>
       The peer presentation user rejected the connection.
       </description>
    </doc>
  </message>

  <message id="51" name="NotSpecified" severity="error">
    <text>Reason not specified</text>
    <doc>
       <description>
       The presentation layer refused the connection or aborted the connection
       for some unspecified reason.
       </description>
    </doc>
  </message>

  <message id="52" name="Congestion" severity="error" args="1">
    <arg position="1">Local reason for congestion</arg>
    <text>Presentation connection refused, congestion %1</text>
    <doc>
       <description>
       The presentation layer refused the connection as a result of the
       temporary shortage of some resource.
       </description>
    </doc>
  </message>

  <message id="53" name="LocalLimit" severity="error">
    <text>Presentation connection refused, local limit exceeded</text>
    <doc>
       <description>
